{
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "never"],
    "no-console": 0,
    "semi": [2, "never"]
  }
}
